{
  "name": "irecruit-skeleton-service",
  "version": "0.0.1",
  "description": "Express webpack skeleton service",
  "scripts": {
    "test": "jest --verbose --detectOpenHandles --forceExit",
    "api-test": "jest --verbose ./tests/api-test --detectOpenHandles --forceExit",
    "coverage": "jest --coverage",
    "lint": "sh ./ops/sh/.eslint.sh",
    "build:dev": "npm run env && webpack --mode development --watch",
    "build": "npm run env && webpack --mode production",
    "start:dev": "nodemon ./dist/server.js",
    "start": "node ./dist/server.js",
    "env": "sh ./ops/sh/.env.sh",
    "gen-chart": "pipeline-utils gen-chart -n $npm_package_name -d $npm_package_description -v $npm_package_version",
    "gen-api-doc": "pipeline-utils gen-api-doc -s ./dist/server.js -o $API_DOC_OUTPUT"
  },
  "keywords": [],
  "author": "AIA",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://AIAIRecruit@dev.azure.com/AIAIRecruit/iRecruitSgDev/_git/irecruit-skeleton-service"
  },
  "pre-commit": [
    "lint",
    "test",
    "coverage"
  ],
  "dependencies": {
    "@babel/runtime-corejs2": "^7.4.3",
    "@aia-digital/server-module": "^1.6.1",
    "@aia-digital/logger-module": "^1.2.0",
    "express": "^4.16.4",
    "express-query-parser": "^1.0.2",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "knex": "^0.16.3",
    "mssql": "^5.0.3",
    "swagger-jsdoc": "^3.2.7",
    "uuid": "^3.3.2",
    "dotenv": "^7.0.0",
    "http-status-codes": "^1.3.2",
    "source-map-support": "^0.5.11"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-source-map-support": "^2.0.1",
    "clean-webpack-plugin": "^2.0.1",
    "delay": "^4.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.2.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^24.7.1",
    "nodemon": "^1.18.10",
    "newman": "^4.4.0",
    "pre-commit": "^1.2.2",
    "sinon": "^7.3.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-node-externals": "^1.7.2",
    "wait-on": "^3.2.0"
  }
}
